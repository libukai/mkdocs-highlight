{
  "name": "mkdocs-syntax-highlight",
  "displayName": "MkDocs Syntax Highlight",
  "description": "Highlight for Mkdocs Syntax",
  "version": "0.2.1",
  "publisher": "aikebang",
  "repository": "https://github.com/libukai/mkdocs-highlight",
  "icon": "assets/mkdocs.png",
  "homepage": "https://10k.xyz",
  "license": "MIT",
  "tags": [
    "markdown",
    "mkdocs"
  ],
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "injectTo": [
          "text.html.markdown"
        ],
        "scopeName": "source.mkdocs",
        "path": "./syntaxes/mkdocs.tmLanguage.json"
      }
    ],
    "themes": [
      {
          "label": "MkDocs Theme",
          "uiTheme": "vs-dark",
          "path": "./themes/mkdocs.tmLanguage.json"
      }
  ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
